{
  "name": "jammerfrontend",
  "version": "1.0.0",
  "main": "tailwind.config.js",
  "scripts": {
    "css:build": "npx tailwindcss -i ./wwwroot/css/style.css -o ./wwwroot/css/styles.css --minify",
    "modern:build": "npx tailwindcss -i ./wwwroot/css/modern-style.css -o ./wwwroot/dist/output-tailwind.css",
    "modern:watch": "npx tailwindcss -i ./wwwroot/css/modern-style.css -o ./wwwroot/dist/output-tailwind.css --watch",
    "build": "npm run modern:build",
    "dev": "npm run modern:watch"
  },
