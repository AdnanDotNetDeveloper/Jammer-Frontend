@{
    ViewData ["Title"] = "WishList Page";
}
<div class="breadcrumb-block style-shared">
    <div class="breadcrumb-main bg-linear overflow-hidden">
        <div class="container lg:pt-[134px] pt-24 pb-10 relative">
            <div class="main-content w-full h-full flex flex-col items-center justify-center relative z-[1]">
                <div class="text-content">
                    <div class="heading2 text-center">Wishlist</div>
                    <div class="link flex items-center justify-center gap-1 caption1 mt-3">
                        <a href="index.html">Homepage</a>
                        <i class="ph ph-caret-right text-sm text-secondary2"></i>
                        <div class="text-secondary2 capitalize">Wishlist</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>

<div class="wishlist-block lg:py-20 md:py-14 py-10">
    <div class="container">
        <div class="list-product-block">

            <div class="list-product hide-product-sold grid lg:grid-cols-4 sm:grid-cols-3 grid-cols-2 sm:gap-[30px] gap-[20px] mt-7"></div>
        </div>
    </div>
</div>
<div id="popup-message" class="fixed bg-green-500 text-white p-4 rounded-lg hidden"></div>


<div id="cart-container" class="list grid xl:grid-cols-4 lg:grid-cols-4 md:grid-cols-3 grid-cols-2 sm:gap-[30px] gap-[20px] md:mt-10 mt-6" style="margin-bottom:100px">
  
</div>

<script src="https://cdn.jsdelivr.net/npm/js-cookie@3.0.5/dist/js.cookie.min.js"></script>
<script src="/js/wishlist.js"></script>